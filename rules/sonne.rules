rule "Sonnenuntergang"
when
    Channel 'astro:sun:home:set#event' triggered START
then
    abendlicht.sendCommand("ON")
    UpdatePresenceLivingRoomTime.postUpdate(180)
    UpdatePresenceLivingRoom.sendCommand("ON")
end
